@Article{Gross1998a,
  author     = {Gross, J. J.},
  journal    = {Journal of Personality and Social Psychology},
  title      = {Antecedent- and response-focused emotion regulation: {Divergent} consequences for experience, expression, and physiology},
  year       = {1998},
  issn       = {0022-3514},
